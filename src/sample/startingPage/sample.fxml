               <children>
                  <Button fx:id="buyCarBtn" layoutX="14.0" layoutY="20.0" mnemonicParsing="false" prefHeight="15.0" prefWidth="126.0" text="Купить " textAlignment="CENTER" />
                  <Button fx:id="sellCarBtn" layoutX="232.0" layoutY="20.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="127.0" text="Продать" textAlignment="CENTER" />
               </children>
            </AnchorPane>
